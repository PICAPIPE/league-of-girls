@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

{{baseSelector}} {
  -webkit-font-smoothing: antialiased;
  display: inline-block;
  font-style: normal;
  font-variant: normal;
  text-rendering: auto;
  line-height: 1;
}

{{baseSelector}}:before {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	vertical-align: top;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
